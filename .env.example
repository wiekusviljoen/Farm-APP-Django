# Example environment variables for Farm-APP-Django
# Configure an external feed price provider (optional)
FEED_PRICE_API_URL=https://api.example.com/v1/feed-price
# Either provide the API key as a header (default: Authorization: Bearer <key>)
FEED_PRICE_API_KEY=your_api_key_here
# Or specify a query param name to pass the key: FEED_PRICE_API_KEY_QUERY_PARAM=api_key
# JSON path to the price value (dot-separated)
FEED_PRICE_JSON_PATH=price
# If your provider needs a different header name (e.g. 'X-API-KEY')
FEED_PRICE_API_KEY_HEADER_NAME=Authorization
# Optional prefix for header value (default 'Bearer ')
FEED_PRICE_API_KEY_PREFIX=Bearer 
# Enable or disable autodiscovery against public providers
FEED_PRICE_AUTODISCOVER=True
# When no provider is reachable, demo mode will return randomized prices
FEED_PRICE_DEMO=True
